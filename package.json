{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": " cross-env NODE_ENV=development nest start --watch ",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "migrate:prod": "node-pg-migrate up --envPath .env.production",
    "migrate:dev": "node-pg-migrate up --envPath .env.development",
    "compose:up": "docker compose -f ./docker-compose.yml up",
    "compose:build": "docker compose -f ./docker-compose.yml up --build",
    "compose:stop": "docker compose -f ./docker-compose.yml stop",
    "compose:start": "docker compose -f ./docker-compose.yml start",
    "compose:down": "docker compose -f ./docker-compose.yml down",
    "compose:prod:build": "docker compose -f ./docker-compose.prod.yml up --build",
    "compose:prod:stop": "docker compose -f ./docker-compose.prod.yml stop",
    "compose:prod:start": "docker compose -f ./docker-compose.prod.yml start",
    "compose:prod:down": "docker compose -f ./docker-compose.prod.yml down",
    "commitlint": "commitlint --edit",
    "prepare": "husky"
  },
  "dependencies": {
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/common": "^11.1.11",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.11",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/platform-express": "^11.1.12",
    "@nestjs/swagger": "^11.2.4",
    "bcryptjs": "^3.0.2",
    "cross-env": "^10.1.0",
    "db-migrate": "^0.11.14",
    "ioredis": "^5.9.2",
    "nestjs-zod": "^5.0.1",
    "pg": "^8.16.3",
    "redis": "^5.9.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "uuid": "^13.0.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.39.2",
    "@nestjs/cli": "^11.0.16",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.1.12",
    "@types/express": "^5.0.0",
    "@types/node": "^25.0.9",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^17.0.0",
    "husky": "^9.1.7",
    "node-pg-migrate": "^8.0.4",
    "prettier": "^3.8.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^1.7.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0",
    "unplugin-swc": "^1.5.9",
    "vite-tsconfig-paths": "^6.0.4",
    "vitest": "^4.0.17"
  }
}
